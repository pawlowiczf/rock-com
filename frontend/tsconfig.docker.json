{
  "compilerOptions": {
    "moduleResolution": "node",
    "target": "ES2022",
    "module": "ESNext",
    "jsx": "react-jsx",
    "skipLibCheck": true,
    "noEmit": true,
    "allowImportingTsExtensions": true,
    "allowSyntheticDefaultImports": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
  }
}